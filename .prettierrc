{
  "singleQuote": true,
  "semi": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "printWidth": 130,
  "plugins": ["prettier-plugin-imports"],
  "importOrder": ["^@vercube/(.*)$", "<BUILTIN_MODULES>", "<THIRD_PARTY_MODULES>", "^[./]", "<TYPES>"],
  "importOrderParserPlugins": ["typescript", "decorators-legacy"]
}
