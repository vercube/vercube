{
  "name": "playground",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "description": "Playground for Vercube framework",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build": "vercube build",
    "build:serverless": "vercube build --entry ./src/serverless.ts",
    "dev": "vercube dev",
    "start": "node ./dist/index.mjs",
    "start:serverless": "serverless offline --stage local --noPrependStageInUrl --host 0.0.0.0"
  },
  "devDependencies": {
    "@vercube/cli": "workspace:*"
  },
  "dependencies": {
    "@vercube/auth": "workspace:*",
    "@vercube/core": "workspace:*",
    "@vercube/di": "workspace:*",
    "@vercube/h3": "workspace:*",
    "@vercube/logger": "workspace:*",
    "@vercube/schema": "workspace:*",
    "@vercube/storage": "workspace:*",
    "@vercube/serverless": "workspace:*",
    "@vercube/ws": "workspace:*",
    "serverless": "3",
    "serverless-offline": "14.4.0",
    "tslib": "2.8.1",
    "zod": "4.0.17"
  }
}