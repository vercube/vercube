{
  "name": "@vercube/cli",
  "version": "0.0.1-alpha.18",
  "description": "CLI module for Vercube framework",
  "packageManager": "pnpm@9.10.0",
  "repository": "@vercube/cli",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "vercube": "./dist/index.cjs"
  },
  "dependencies": {
    "@vercube/devkit": "workspace:*",
    "citty": "0.1.6",
    "consola": "3.4.2",
    "giget": "2.0.0",
    "nypm": "0.6.0",
    "pathe": "2.0.3",
    "std-env": "3.9.0",
    "tinyexec": "1.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
