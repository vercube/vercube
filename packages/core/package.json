{
  "name": "@vercube/core",
  "version": "0.0.2",
  "description": "Core module for Vercube framework",
  "packageManager": "pnpm@9.10.0",
  "repository": "@vercube/core",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@standard-schema/spec": "1.0.0",
    "@vercube/di": "workspace:*",
    "@vercube/logger": "workspace:*",
    "c12": "3.0.3",
    "defu": "6.1.4",
    "pathe": "2.0.3",
    "rou3": "0.6.1",
    "srvx": "0.6.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.1.2",
    "vitest": "^3.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
