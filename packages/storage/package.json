{
  "name": "@vercube/storage",
  "version": "0.0.21",
  "description": "Storage module for Vercube framework",
  "packageManager": "pnpm@10.13.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercube/vercube.git",
    "directory": "packages/storage"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.mjs",
