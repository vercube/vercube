    "./drivers/MemoryStorage": "./dist/Drivers/MemoryStorage.mjs",
    "./drivers/S3Storage": "./dist/Drivers/S3Storage.mjs",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "files": [
